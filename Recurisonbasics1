package com.recursion;
//print no 1 to n in decreasing order
public class Recursionbasics {
    public static void printDec(int n)  //recursive func
    {
        if(n==1){
            System.out.println(n);
            return;
        }
        System.out.println(n+"");
        printDec(n-1); //calling inner func
    }

    public static void main(String[] args) {
       int n=10;
        printDec(n);//calling from main func
    }
}
